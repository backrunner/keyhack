{
  "name": "keyhack",
  "version": "0.1.0",
  "description": "Use proxy to bind functions to combo key",
  "main": "dist/keyhack.umd.js",
  "module": "dist/keyhack.esm.js",
  "typings": "dist/types/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "cross-env NODE_ENV=dev rollup -c rollup.config.ts -w",
